{
  "name": "rosnerzh",
  "version": "0.1.0",
  "private": false,
  "dependencies": {
    "antd": "^3.7.3",
    "dotenv": "^6.0.0",
    "mobx": "^5.0.3",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-mobx": "^0.0.3",
    "react-motion": "^0.5.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "react-scroll-parallax": "^1.3.5",
    "react-yandex-maps": "^2.2.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/sass/ -o src/css",
    "watch-css": "npm run build-css && node-sass-chokidar src/sass/ -o src/css --watch --recursive",
    "start-js": "react-scripts start dotenv_config_path=.env",
    "build-js": "react-scripts build",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
