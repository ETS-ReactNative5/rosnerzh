{
  "name": "rosnerzh",
  "version": "3.0.1",
  "private": false,
  "author": "vk.com/didex",
  "license": "ISC",
  "dependencies": {
    "antd": "^3.7.3",
    "axios": "^0.18.0",
    "babel-plugin-styled-components": "^1.5.1",
    "dotenv": "^6.0.0",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.5",
    "mobx-react-devtools": "^6.0.2",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.1",
    "react-app-rewire-mobx": "^1.0.8",
    "react-app-rewired": "^1.5.2",
    "react-dom": "^16.4.1",
    "react-input-range": "^1.3.0",
    "react-motion": "^0.5.2",
    "react-rangeslider": "^2.2.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "react-scroll-parallax": "^1.3.5",
    "react-yandex-maps": "^2.2.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/sass/ -o src/css",
    "watch-css": "npm run build-css && node-sass-chokidar src/sass/ -o src/css --watch --recursive",
    "start-js": "react-app-rewired start dotenv_config_path=.env",
    "build-js": "react-app-rewired build",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject"
  }
}
